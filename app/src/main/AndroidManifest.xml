<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voms">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Vestibular/Ocular-Motor Screening"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VMSTest"></activity>
        <activity android:name=".Results" />
        <activity android:name=".VORVertical" />
        <activity android:name=".VORHorizontal" />
        <activity android:name=".Convergence" />
        <activity android:name=".SaccadesVertical" />
        <activity android:name=".SaccadesHorizontal" />
        <activity android:name=".SmoothPursuits" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>